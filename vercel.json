{
  "outputDirectory": ".",
  "headers": [
    {
      "source": "/(.*)\\.(js|css|png|jpg|jpeg|svg|webp|ico|gif|woff2|woff|ttf)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Encoding",
          "value": "br"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, s-maxage=2592000, stale-while-revalidate=86400"
        }
      ]
    }
  ]
}

